{
  "private": true,
  "name": "streamers_movies",
  "packageManager": "npm@9.3.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "eslint \"apps/**/*.{js,mjs,cjs}\"",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint:fix": "eslint \"src/**/*.{js,mjs,cjs}\" --fix"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-kit": "^10.30.0",
    "tsconfig": "*",
    "turbo": "latest",
    "prettier": "^3.2.5"
  }
}